<script src="../jquery.js"></script>
<script src="../jquery.tmpl.js"></script>
<script src="../plugins/tmpl.extend.js"></script>

<script id='tmpl1' type="text/html">
    <p>
        <a href="${url}">${name}</a> has been to these cities.
    </p>
</script>

<script id="tmpl2" type="text/html">
{{extend tmpl1}}
<ul>
{{each(i,city) cities}}
    <li> ${$i}) City: ${city}</li>
{{/each}}
</ul>
</script>

<script>
jQuery(function(){
	var dataObject = {
        name: function() {
			return this.firstName + " " + this.lastName;
		},
        firstName: "John",
		lastName: "Resig",
        url: "http://ejohn.org/",
		cityJoin: function() {
			return this.cities.join(", ");
		},
    	cities: [
            	"Boston, MA",
            	"San Francisco, CA"
    	]
	};
	
	jQuery('#tmpl2').render( dataObject ).appendTo("div");

});
</script>

<div></div>
